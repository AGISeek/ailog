{
	"name": "ailog",
	"displayName": "%displayName%",
	"description": "%description%",
	"version": "1.0.0",
	"publisher": "qiqi.zhou@gmail.com",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/AGISeek/ailog"
	},
	"engines": {
		"vscode": "^1.85.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ailog.installGitHook",
				"title": "%command.installGitHook%"
			},
			{
				"command": "ailog.toggleAiGenerated",
				"title": "%command.toggleAiGenerated%"
			},
			{
				"command": "ailog.enableRealtimeDetection",
				"title": "%command.enableRealtimeDetection%"
			},
			{
				"command": "ailog.showDashboard",
				"title": "%command.showDashboard%"
			},
			{
				"command": "ailog.processCommit",
				"title": "%command.processCommit%"
			},
			{
				"command": "ailog.interactiveCommitCheck",
				"title": "%command.interactiveCommitCheck%"
			},
			{
				"command": "ailog.configureAiAttribution",
				"title": "%command.configureAiAttribution%"
			}
		],
		"configuration": {
			"title": "AIlog",
			"properties": {
				"ailog.aiAttribution.name": {
					"type": "string",
					"default": "Cursor AI",
					"description": "%configuration.aiAttribution.name.description%"
				},
				"ailog.aiAttribution.email": {
					"type": "string",
					"default": "cursor-ai@company.com",
					"description": "%configuration.aiAttribution.email.description%"
				},
				"ailog.detection.confidenceThreshold": {
					"type": "number",
					"default": 70,
					"minimum": 0,
					"maximum": 100,
					"description": "%configuration.detection.confidenceThreshold.description%"
				},
				"ailog.detection.enableTimeProximity": {
					"type": "boolean",
					"default": true,
					"description": "%configuration.detection.enableTimeProximity.description%"
				},
				"ailog.detection.enablePatternMatching": {
					"type": "boolean",
					"default": true,
					"description": "%configuration.detection.enablePatternMatching.description%"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint src --ext ts",
		"package": "vsce package"
	},
	"dependencies": {
		"chart.js": "^4.5.0",
		"simple-git": "^3.22.0",
		"sqlite3": "^5.1.7"
	},
	"devDependencies": {
		"@types/node": "18.x",
		"@types/sqlite3": "^3.1.11",
		"@types/vscode": "^1.85.0",
		"@typescript-eslint/eslint-plugin": "^6.15.0",
		"@typescript-eslint/parser": "^6.15.0",
		"eslint": "^8.56.0",
		"typescript": "^5.3.3",
		"vsce": "^2.15.0"
	}
}
